<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.guet.login.mapper.UserPasswordMapper">
    <select id="findByUserId" resultType="cn.edu.guet.login.domain.UserPassword">
        SELECT * FROM user_password
        WHERE user_id = #{userId}
    </select>
</mapper>